<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.pos2d.webapi.entities.industrialloadgis" table="utilitygis.industrialloadgis">
      <id name="gid">
      	<generator class="identity" />
      </id>
       <property name="companyname" type="string"/>
	   <property name="areaname" type="string"/>
	   <property name="currentdemandkw" type="double"/>
	   <property name="cityname" type="string"/>
	   <property name="premiseaddress" type="string"/>
	   <property name="xcordinate" type="double"/>
	   <property name="ycordinate" type="double"/>
	   <property name="longitude" type="double"/>
	   <property name="latitude" type="double"/>
	   <property name="geom" type="org.hibernate.spatial.GeometryType"/>
	   	
	   </class>

</hibernate-mapping>



	
	
	
	
 
  
	
	