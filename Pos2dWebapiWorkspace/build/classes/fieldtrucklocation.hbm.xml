<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.pos2d.webapi.entities.fieldtrucklocation" table="utilitydiscomasset.fieldtrucklocation">
      <id name="gid">
      	<generator class="identity" />
      </id>
       <property name="regno" type="string"/>
         <property name="fieldmobileid" type="string"/>
         <property name="ts" type="timestamp"/>
         <property name="totalalertsnoticed" type="integer"/>
          <property name="xcordinate" type="double"/>
          <property name="ycordinate" type="double"/>
          <property name="longitude" type="double"/>
          <property name="latitude" type="double"/>  
       
   </class>
</hibernate-mapping>