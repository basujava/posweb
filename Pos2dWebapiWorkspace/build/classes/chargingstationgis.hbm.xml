<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.pos2d.webapi.entities.chargingstationgis" table="utilitygis.chargingstationgis">
      <id name="gid">
      	<generator class="identity" />
      </id>
       <property name="csname" type="string"/>
	   <property name="areatype" type="string"/>
	   <property name="areaname" type="string"/>
	   <property name="cityname" type="string"/>
	   <property name="fulladdress" type="string"/>
	   <property name="lastpricesignal" type="double"/>
	   <property name="currentcarqueue" type="integer"/>
	   <property name="xcordinate" type="double"/>
	   <property name="ycordinate" type="double"/>
	   <property name="longitude" type="double"/>
	   <property name="latitude" type="double"/>
	   <property name="geom" type="org.hibernate.spatial.GeometryType"/>
	   
	  </class>
</hibernate-mapping>   				
				